{
	"compilerOptions": {
	  "module": "esnext",
	  "target": "ESNext",
	  "lib": ["dom", "esnext"], 
	  "moduleResolution": "node",
	  "strict": true,
	  "esModuleInterop": true
	},
	"include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.svelte"],
	"exclude": ["node_modules/*", "__sapper__/*", "public/*"]
  }
  