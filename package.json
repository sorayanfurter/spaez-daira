{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "build:css": "postcss src/css/main.css -o public/build/tailwind.css",
    "watch:css": "postcss src/css/main.css -o public/build/tailwind.css -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@tsconfig/svelte": "^5.0.2",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "rollup": "^3.15.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.3",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "sirv-cli": "^2.0.0"
  }
}
