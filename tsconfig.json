{
	"compilerOptions": {
	  "module": "esnext",
	  "target": "ESNext",
	  "lib": ["dom", "esnext"], 
	  "moduleResolution": "node",
	  "strict": true,
	  "esModuleInterop": true
	},
	"include": ["FrontEnd/src/**/*.ts", "FrontEnd/src/**/*.d.ts", "FrontEnd/src/**/*.tsx", "FrontEnd/src/**/*.svelte"],
	"exclude": ["node_modules/*", "__sapper__/*", "public/*"]
  }
  